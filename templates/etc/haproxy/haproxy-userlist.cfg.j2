{% if haproxy_userlist is defined and haproxy_userlist|length %}
##########################
#       USERLIST         #
##########################
    {% for dict_item in haproxy_userlist %}
        {% for name, value in dict_item.items() %}
userlist {{ name }}
            {% for group in value.groups %}
    group       {{ group }}
            {% endfor %}
            {% for user in value.users %}
    user        {{ user }}
            {% endfor %}
        {% endfor %}

    {% endfor %}
{% endif %}
